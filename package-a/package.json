{
	"name": "package-a",
	"exports": {
		".": {
			"source": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./namespace": {
			"source": "./src/namespace.ts",
			"default": "./dist/namespace.js"
		},
		"./proxy-internal": {
			"source": "./src/proxy-internal.ts",
			"default": "./dist/proxy-internal.js"
		}
	},
	"imports": {
		"#internal": {
			"source": "./src/internal/index.ts",
			"default": "./dist/internal/index.js"
		}
	},
	"scripts": {
		"build": "tsc --project ./tsconfig.src.json"
	},
	"dependencies": {
		"typescript": "^5.6.3"
	},
	"devDependencies": {
		"package-a": "file:."
	}
}
